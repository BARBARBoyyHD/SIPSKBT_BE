{
    "version": 2,
    "builds": [
      { "src": "*.js", "use": "@vercel/node" }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ],
    "routes":[
      {
        "src":"/(.*)",
        "dest":"/index.js",
        "method":["GET","HEAD","PUT","PATCH","POST","DELETE"],
        "headers":{
          "Access-Control-Allow-Origin":"*"
        }
      }
    ]
  }
  